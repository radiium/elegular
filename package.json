{
  "name": "elegular",
  "version": "9.0.0",
  "license": "MIT",
  "description": "Electron + Angular 9 boilerplate",
  "homepage": "https://github.com/radiium/elegular",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/elegular.git"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "boilerplate",
    "electron",
    "angular 9"
  ],
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:electron": "tsc -p src-electron/ && npx copyfiles --flat package.json src-electron/postcss.config.js dist/",
    "build:all": "npm run build:prod && npm run build:electron",
    "start": "npm-run-all -p serve electron:reload",
    "start:web": "npm run serve -- -o",
    "start:prod": "npm run build:prod && npm run build:electron && electron dist/",
    "electron:reload": "npm run build:electron && wait-on http-get://localhost:4200/ && electron dist/ --dev",
    "release:all": "npm run release -- -lmw",
    "release": "npm run build:all && electron-builder build --config electron-builder.yml"
  },
  "private": true,
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/animations": "^9.1.5",
    "@angular/cli": "~9.1.4",
    "@angular/common": "^9.1.5",
    "@angular/compiler": "^9.1.5",
    "@angular/compiler-cli": "~9.1.5",
    "@angular/core": "^9.1.5",
    "@angular/forms": "^9.1.5",
    "@angular/language-service": "~9.1.5",
    "@angular/platform-browser": "^9.1.5",
    "@angular/platform-browser-dynamic": "^9.1.5",
    "@angular/router": "^9.1.5",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.2",
    "copyfiles": "^2.3.0",
    "devtron": "^1.4.0",
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "electron-context-menu": "^2.2.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ngx-electron": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.4",
    "rxjs": "^6.6.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "wait-on": "^5.1.0",
    "zone.js": "~0.10.2"
  }
}
