{
  "name": "elegular",
  "version": "8.0.0",
  "license": "MIT",
  "description": "Electron + Angular 7 boilerplate",
  "homepage": "https://github.com/radiium/elegular",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/elegular.git"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "electron",
    "angular 8"
  ],
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:electron": "tsc -p src-electron/ && npx copyfiles --flat package.json src-electron/postcss.config.js dist/",
    "build:all": "npm run build:prod && npm run build:electron",
    "start": "npm-run-all -p serve electron:reload",
    "start:web": "npm run serve -- -o",
    "start:prod": "npm run build:prod && npm run build:electron && electron dist/",
    "electron:reload": "npm run build:electron && wait-on http-get://localhost:4200/ && electron dist/ --dev",
    "release:all": "npm run release -- -lmw",
    "release": "npm run build:all && electron-builder build --config electron-builder.yml"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular/animations": "^8.2.14",
    "@angular/cli": "~8.3.23",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/language-service": "~8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "copyfiles": "^2.2.0",
    "devtron": "^1.4.0",
    "electron": "^7.1.9",
    "electron-builder": "^22.2.0",
    "electron-context-menu": "^0.15.2",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngx-electron": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rxjs": "^6.5.4",
    "ts-node": "~7.0.0",
    "tslib": "^1.10.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "wait-on": "^4.0.0",
    "zone.js": "^0.10.2"
  }
}
