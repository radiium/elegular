{
  "name": "elegular",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Electron angular boilerplate",
  "homepage": "https://github.com/radiium/elegular",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/elegular.git"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "electron",
    "angular 6",
    "typescript"
  ],
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:electron": "tsc -p src-electron/ && npx copyfiles --flat package.json src-electron/postcss.config.js dist/",
    "build:all": "npm run build:prod && npm run build:electron",
    "start": "npm-run-all -p serve electron:reload",
    "start:web": "npm run serve -- -o",
    "start:prod": "npm run build:prod && npm run build:electron && electron dist/",
    "electron:reload": "npm run build:electron && wait-on http-get://localhost:4200/ && electron dist/ --dev",
    "release:all": "npm run release -- -lmw",
    "release": "npm run build:all && electron-builder build --config electron-builder.yml"
  },
  "dependencies": {
    "electron-context-menu": "^0.12.1",
    "electron-json-storage": "^4.1.6",
    "electron-log": "^3.0.5",
    "electron-updater": "^4.0.6",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/cli": "~7.3.9",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@mdi/font": "^3.6.95",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.9.4",
    "angular2-uuid": "^1.1.1",
    "codelyzer": "^5.0.1",
    "copyfiles": "^2.1.0",
    "core-js": "^3.1.2",
    "devtron": "^1.4.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "electron-builder-squirrel-windows": "^20.42.0",
    "electron-icon-maker": "0.0.4",
    "electron-reload": "^1.4.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "merge-stream": "^2.0.0",
    "ngx-electron": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "protractor": "^5.4.2",
    "rxjs": "^6.5.2",
    "ts-node": "^8.1.1",
    "tslint": "^5.16.0",
    "typescript": "~3.2.4",
    "wait-on": "^3.2.0",
    "zone.js": "^0.9.1"
  }
}
